

<% semantic_form_for(@section) do |form|  %>
  <% form.inputs do %>
    <%= form.input :title %>
    <%= form.input :body %>
    <%= form.input :video %>
  <% end %>
  <% form.buttons do %>
    <%= form.commit_button %>
    <%= link_to 'Cancelar', section_videos_path(@section) %>
  <% end %>
<% end %>